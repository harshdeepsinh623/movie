{
  "extends": [
    "development"
  ],
  "hints": {
    "axe/forms": [
      "default",
      {
        "select-name": "off",
        "label": "off"
      }
    ],
    "axe/parsing": [
      "default",
      {
        "duplicate-id": "off",
        "duplicate-id-active": "off"
      }
    ],
    "axe/aria": [
      "default",
      {
        "aria-required-children": "off"
      }
    ],
    "button-type": "off",
    "axe/name-role-value": [
      "default",
      {
        "button-name": "off",
        "link-name": "off"
      }
    ],
    "detect-css-reflows/composite": "off",
    "detect-css-reflows/paint": "off",
    "compat-api/css": [
      "default",
      {
        "ignore": [
          "scrollbar-width",
          "scrollbar-width: thin",
          "scrollbar-color",
          "backdrop-filter",
          "position: -webkit-sticky"
        ]
      }
    ],
    "axe/structure": [
      "default",
      {
        "list": "off"
      }
    ]
  }
}